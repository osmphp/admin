{
    "name": "osmphp/admin",
    "description": "A package for defining data structures using PHP 8 classes and attributes, and getting fully functioning Admin Panel and API.",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "osmphp/framework": "^0.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "files": [
            "src/Formulas/functions.php",
            "src/Storages/functions.php"
        ],
        "psr-4": {
            "Osm\\Admin\\": "src/",
            "Osm\\Admin\\Tools\\": "tools/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osm\\Admin\\Tests\\": "tests/",
            "Osm\\Admin\\TestsMigrations\\": "tests_migrations/",
            "Osm\\Admin\\Samples\\": "samples/"
        }
    }
}
